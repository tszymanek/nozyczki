<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}‚úÅ.ws{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets
                'bundles/nozyczkishortener/css/vendor/*'
                'bundles/nozyczkishortener/css/flat-ui.min.css'
            'bundles/nozyczkishortener/css/style.css'
            filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {% javascripts
                'bundles/nozyczkishortener/js/vendor/*'
                'bundles/nozyczkishortener/js/*'
                'bundles/nozyczkishortener/js/core/*'
            %}
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
            <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
